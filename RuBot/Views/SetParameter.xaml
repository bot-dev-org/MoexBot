<Window x:Class="RuBot.Views.SetParameter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Set Parameter" Height="125" Width="452">
    <Grid Height="88">
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" x:Name="StrategiesBox" VerticalAlignment="Top" Width="98" SelectionChanged="StrategiesBoxSelectionChanged" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="116,12,0,0" x:Name="ParametersBox" VerticalAlignment="Top" Width="175" SelectionChanged="ParametersBoxSelectionChanged" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="298,12,0,0" x:Name="ValueBox" VerticalAlignment="Top" Width="120" />
        <Button Content="Set" Height="23" HorizontalAlignment="Left" Margin="171,52,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="Button1Click" />
    </Grid>
</Window>
